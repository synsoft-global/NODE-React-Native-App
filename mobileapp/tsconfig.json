{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
   
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true ,    
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "utils/*": ["src/utils/*"],
      "service/*": ["src/service/*"],
      "screens/*": ["src/screens/*"],
      "appConfig/*": ["appConfig"],
      "components/*": ["src/components/*"],
      "globalStyle/*": ["src/globalStyle/*"],
      "assets/*": ["./assets"],
    }
  },
}

